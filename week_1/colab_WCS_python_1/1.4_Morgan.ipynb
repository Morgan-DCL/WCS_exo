      "source": [
        "liste = \"Excellent work\"\n",
        "lettre = \"e\"\n",
        "lettre = lettre.lower()\n",
        "compteur_e = 0\n",
        "for phrase in liste:\n",
        "    if lettre in phrase:\n",
        "        compteur_e += phrase.count(lettre)\n",
        "print(\"Il y a {} 'e' dans la phrase.\".format(compteur_e))\n"
      ]
    }
